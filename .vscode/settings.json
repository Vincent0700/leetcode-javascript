{
    "editor.formatOnSave": true,
    "eslint.enable": true,
    "eslint.format.enable": true,
    "eslint.alwaysShowStatus": true,
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust"
    },
    "files.associations": {
        "__locale": "cpp",
        "__string": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "vector": "cpp",
        "regex": "cpp"
    },
    "rust.rust-analyzer": {
        "releaseTag": "nightly",
        "path": null
    }
}